{
  "name": "reslick",
  "version": "1.0.1",
  "description": "This is the simple carousel using React + Typescript + styled-component.",
  "homepage": "https://github.com/hey3/reslick#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hey3/reslick.git"
  },
  "bugs": {
    "url": "https://github.com/hey3/reslick/issues"
  },
  "keywords": [
    "react",
    "react-component",
    "typescript",
    "carousel"
  ],
  "license": "MIT",
  "author": "hey3",
  "main": "dist/umd/index.js",
  "module": "dist/esm/index.js",
  "typings": "dist/umd/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "yarn build:esm && yarn build:umd",
    "build:esm": "tsc -p tsconfig.build.esm.json",
    "build:umd": "tsc -p tsconfig.build.umd.json",
    "build-storybook": "build-storybook",
    "format": "prettier --write './src/**/*.{ts,tsx,json}'",
    "lint": "yarn lint:eslint && yarn lint:style && yarn lint:tsc",
    "lint:eslint": "eslint 'src/**/*.ts{,x}'",
    "lint:style": "stylelint 'src/**/*.ts{,x}'",
    "lint:tsc": "tsc --noEmit",
    "storybook": "start-storybook -p 6006",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:virtual-update": "jest --updateSnapshot",
    "clean": "rimraf dist",
    "prepublishOnly": "yarn clean && yarn build"
  },
  "dependencies": {},
  "devDependencies": {
    "@storybook/addon-a11y": "^6.0.28",
    "@storybook/addon-essentials": "^6.0.28",
    "@storybook/addon-knobs": "^6.0.28",
    "@storybook/addons": "^6.0.28",
    "@storybook/react": "^6.0.28",
    "@storybook/theming": "^6.0.28",
    "@types/enzyme": "^3.10.7",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.15",
    "@types/react": "^16.9.55",
    "@types/react-dom": "^16.9.9",
    "@types/react-test-renderer": "^16.9.3",
    "@types/styled-components": "^5.1.4",
    "@typescript-eslint/parser": "^4.6.1",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.6.3",
    "jest-styled-components": "^7.0.3",
    "prettier": "^2.1.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-test-renderer": "^16.14.0",
    "rimraf": "^3.0.2",
    "styled-components": "^5.2.0",
    "stylelint": "^13.7.2",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "ts-jest": "^26.4.4",
    "typescript": "^4.0.5"
  },
  "peerDependencies": {
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "styled-components": "^5.2.0"
  }
}
